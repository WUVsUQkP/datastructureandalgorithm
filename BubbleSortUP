def bubble_sort(alist):
    count = 0
    flag = 0
    border = len(alist) - 1
    for j in range(len(alist)-1):
        swapped = False
        print(alist)
        
        for i in range(0,border):
            count += 1
            if alist[i] > alist[i+1]:
                swapped = True
                alist[i],alist[i+1] = alist[i+1],alist[i]
                flag = j
        if not swapped:
            break
        border = flag
    print(f"总循环次数{count}")
    return alist
